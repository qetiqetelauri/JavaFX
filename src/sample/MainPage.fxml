<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox fx:id="parent" prefHeight="680.0" prefWidth="1200.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MainPageController">
    <children>
        <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="680.0" prefWidth="100.0" style="-fx-background-color: #121920;">
            <children>
                <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: #2BBFBD;">
                    <children>
                        <ImageView fitHeight="50.0" fitWidth="80.0" pickOnBounds="true" smooth="false">
                            <image>
                                <Image url="@../img/1-300x227.png" />
                            </image>
                        </ImageView>
                    </children>
                </HBox>
                <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
                    <children>
                  <Button fx:id="HomeButton1" mnemonicParsing="false" onAction="#show_MainPage" text="Home" textFill="WHITE" />
                  <Button fx:id="UploadButton1" mnemonicParsing="false" onAction="#show_UploadPage" text="Upload" textFill="WHITE">
                     <VBox.margin>
                        <Insets top="30.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="Button1" mnemonicParsing="false" onAction="#Request_Click" text="Request" textFill="WHITE">
                     <VBox.margin>
                        <Insets top="30.0" />
                     </VBox.margin>
                  </Button>
                    </children>
                </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" />
                <VBox alignment="BOTTOM_CENTER" layoutX="10.0" layoutY="110.0" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
                    <VBox.margin>
                        <Insets bottom="10.0" />
                    </VBox.margin>
                </VBox>
            </children>
        </VBox>
        <VBox style="-fx-background-color: #eee;" HBox.hgrow="ALWAYS">
            <children>
                <HBox alignment="CENTER_LEFT" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="100.0" prefWidth="200.0" style="-fx-background-color: #fff;">
                    <padding>
                        <Insets left="10.0" />
                    </padding>
               <children>
                  <JFXButton fx:id="LogOutButton" mnemonicParsing="false" onAction="#LogOut" prefHeight="17.0" prefWidth="73.0" style="-fx-background-color: #151928;" text="Log out" textFill="#a2a0ab">
                     <HBox.margin>
                        <Insets left="1000.0" />
                     </HBox.margin>
                  </JFXButton>
               </children>
                </HBox>
            <BorderPane fx:id="MainBorder" prefHeight="580.0" prefWidth="740.0" />
            </children>
        </VBox>
    </children>
</HBox>
